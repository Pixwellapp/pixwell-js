{
    "name": "pixwell",
    "version": "1.0.0",
    "description": "Official JavaScript/TypeScript SDK for Pixwell Screenshot API",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": ["dist", "README.md", "LICENSE"],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint src --ext .ts",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "pixwell",
        "screenshot",
        "api",
        "web",
        "capture",
        "puppeteer",
        "playwright"
    ],
    "author": "Pixwell",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pixwell/pixwell-js.git"
    },
    "homepage": "https://pixwell.dev",
    "bugs": {
        "url": "https://github.com/pixwell/pixwell-js/issues"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "tsup": "^8.0.1",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
